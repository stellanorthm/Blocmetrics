<h1>All Applications</h1>
<div class="row">
  <div class="col-md-8">
 <% @registered_applications.each do | registered_application | %>
   <div class="media">
     <div class="media-body">
       <h4 class="media-heading">
         <%= link_to registered_application.name, registered_application %>
       </h4>
     </div>
   </div>
 <% end %>
 </div>
 <div class="col-md-4">
   <%= link_to "Create New", new_registered_application_path, class: 'btn btn-success' %>
 </div>
</div>
